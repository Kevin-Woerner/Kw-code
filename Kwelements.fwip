{MACRO CNSTMASS <NM>; <AMUWEIGHT>; <ERR> {
   {ELMASX <NM>; <AMUWEIGHT> }
   CONST <NM>MASS_ERR <ERR>; #1
   CONST <NM>ENERGY <NM>MASS * (Kw.LIGHT * Kw.LIGHT); #ML-2T-2
}}

