IMPORT "Kw";

{MACRO ELMASX <SY>; <AMUWEIGHT> {
   CONST <SY>_WEIGHT <AMUWEIGHT>; #1
   CONST <SY>_MASS   <AMUWEIGHT> * Kw.MOLARMASS; #MN-1
   CONST <SY>MASS    <SY>_MASS / Kw.AVOGADRO; #M
}}

{MACRO ELMASS <NAME>; <NUM>; <SY>; <AMUWEIGHT> {
   CONST <NAME>_Z <NUM>;                   #1
   CONST <NAME> <AMUWEIGHT>;               #1
   CONST <SY>_Z <NAME>_Z;                  #1
   {ELMASX <SY>; <AMUWEIGHT> }
}}

{MACRO EL <NAM>; <NUM>; <SY>; <AMU>; <DEN>  ; <MEL>; <BOI>; <HEA>; <EN> {
   {ELMASS <NAM>; <NUM>; <SY>; <AMU> }
   CONST <SY>_DENSITY <DEN> * Kw.DENSITY; #ML-3
   CONST <SY>_MELT <MEL> * Kw.KELVIN;      #K
   CONST <SY>_BOIL <BOI> * Kw.KELVIN;      #K
   CONST <SY>_Cp <HEA> * Kw.SPECIFICHEAT; #L2K-1T-2
   CONST <SY>_En <EN>; #1
}}

