FUNC DBL watervapor_pressure(DBL temp_k):
   # Vapor Pressure of Water at temperature TEMP_K
   # via The Buck equation.
   # Wikipedia::Arden_Buck_equation
   # Wikipedia::Vapour_pressure_of_water
   DBL retvalu;
   DBL temp_c;

   IF temp_k <= 16.1 OR WATER_CRITICAL <= temp_k:
      retvalu = 0.0;
   ELSE:
      temp_c = Kw.k2tempc(temp_k);
      IF 0.0 <= temp_c:
         retvalu = 611.21 * EXP((18.678 - temp_c / 234.5)
                  * (temp_c / (257.14 + temp_c)));
      ELSE:
         retvalu = 611.15 * EXP((23.036 - temp_c / 333.7)
                  * (temp_c / (279.82 + temp_c)));
      IF_END
   IF_END
   RETURN retvalu;
FUNC_END

FUNC DBL watervaporization(DBL temp_k):
   # Enthalpy of vaporization of water at temperature TEMP_K
   # in units of JOULE / MOLE
   DBL retvalu;

   IF WATER_CRITICAL <= temp_k:
      retvalu = 0.0;
   ELSE:
      retvalu = POW((WATER_CRITICAL - temp_k) / (WATER_CRITICAL - 373.15)
               , 0.37) * WATER_VAPORIZATION_JPMOLE;
   IF_END
   RETURN retvalu;
FUNC_END

FUNC DBL waterboiling(DBL pressure_pa):
   # Temperature in Kelvin of Boiling water at atmospheric pressure
   # PRESSURE_PA via the Clausius-Clapeyron equation
   # Wikipedia::Boiling_point
   DBL retvalu;

   IF pressure_pa <= 0.0:
      retvalu = 0.0;
   ELSE:
      retvalu = 1.0 / (1.0 / 373.15 - LN(pressure_pa / Kw.ATM)
               * Kw.GAS / WATER_VAPORIZATION_JPMOLE);
   IF_END
   RETURN retvalu;
FUNC_END

FUNC DBL waterdensity_airfree(DBL temp_k):
   # density of air-free water at TEMP_K between 273.15 K and 373.15 K.
   # see https://www.ncbi.nlm.nih.gov /pmc/articles/PMC4909168/
   DBL temp_c;
   DBL retvalu;

   temp_c = Kw.k2tempc(temp_k);
#   temp_c = 0.0005 + 0.9997333 * temp_c;
   IF temp_c < 5.0 OR 40.0 < temp_c:
      retvalu = ((((((
               - 2.8054253E-10) * temp_c
               + 1.0556302E-7) * temp_c
               - 4.6170461E-5) * temp_c
               - 7.9870401E-3) * temp_c
               + 1.6945176E+1) * temp_c
               + 9.9983952E+2);
      retvalu /= (1.6897850E-2 * temp_c + 1.0);
   ELSE:
      retvalu = (((((
               - 3.821216E-7) * temp_c
               + 6.943248E-5) * temp_c
               - 8.523829E-3) * temp_c
               + 6.32693E-2) * temp_c
               + 9.9985308E+2);
   IF_END
   RETURN retvalu;
FUNC_END

FUNC DBL waterdensity(DBL temp_k):
   # density of air saturated water at TEMP_K between 273.15 K and 373.15 K.
   # see https://www.ncbi.nlm.nih.gov /pmc/articles/PMC4909168/
   #   for 5 C to 40 C
   DBL temp_c;
   DBL retvalu;

   temp_c = Kw.k2tempc(temp_k);
   IF temp_c < 0.0:
      retvalu = 917 - 0.13 * temp_c;
   ELSIF temp_c <= 5.0:
      retvalu = (((
               + 8.7191684845109E-5) * temp_c
               - 9.0329747829960E-3) * temp_c
               + 6.7771627919989E-2) * temp_c
               + 9.9984281840963E+2;
   ELSIF temp_c <= 40.0:
      retvalu = ((((
               - 3.821216E-7) * temp_c
               + 6.943248E-5) * temp_c
               - 8.523829E-3) * temp_c
               + 6.337563E-2) * temp_c
               + 9.9984847E+2 + 0.0059; # 2021-01-12 kdw  correction
   ELSE:
      retvalu = ((((
               - 6.5202248732325E-8) * temp_c
               + 2.5745673934090E-5) * temp_c
               - 6.1868849581972E-3) * temp_c
               + 6.0164627537211E-3) * temp_c
               + 1.0003918667215E+3;
   IF_END
   RETURN retvalu;
FUNC_END

