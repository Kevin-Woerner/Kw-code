{MACRO CNSTSM <NM>; <EVMASS>; <EVERR> {
   CONST <NM>_ENERGY     <EVMASS> * Kw.eV; #ML2T-2
   CONST <NM>MASS       <NM>_ENERGY / (Kw.LIGHT * Kw.LIGHT); #M
   CONST <NM>_MASS      <NM>MASS * Kw.AVOGADRO; #MN-1
   CONST <NM>MASS_ERR   <EVERR> / <EVMASS>; #1
   CONST <NM>_Hz         <NM>_ENERGY / Kw.PLANCK; #T-1
   CONST <NM>_WAVELENGTH Kw.LIGHT / <NM>_Hz; #L
}}

{MACRO CNSTQ <NM>; <EVMASS>; <EVERR>; <Q> {
   {CNSTSM <NM>QUARK ; <EVMASS>; <EVERR> }
   CONST <NM>QUARK_CHARGE <Q> * Kw.ELEMENTARYCHARGE; #TI
}}
